{
  "name": "spotify-dashboard",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "sso": "source .env && aws sso login --sso-session=${AWS_SSO_SESSION}",
    "deploy": "sst deploy",
    "refresh": "sst refresh",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format . --write && biome check . --write --unsafe"
  },
  "dependencies": {
    "sst": "^3",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-popover": "1.0.6",
    "@radix-ui/react-select": "1.2.2",
    "@types/numeral": "2.0.2",
    "@types/spotify-web-api-node": "5.0.7",
    "add": "2.0.6",
    "chart.js": "4.3.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "next": "14.1.0",
    "numeral": "2.0.6",
    "pluralize": "8.0.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.9.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "1.14.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "1.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@types/node": "20.8.10",
    "@types/pluralize": "0.0.30",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.23",
    "typescript": "5.2.2"
  }
}
